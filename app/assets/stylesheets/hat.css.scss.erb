.hat{
  background-color: white;
  border-top: 1px solid rgba(0, 0, 0, 0.1);
  border-bottom: 1px solid rgba(0,0,0,0.1);
  transition: all 0.3s ease;
  .menu-point{
    font-size: 14px;
    font-weight: 400;
    position: relative;
    bottom: 9px;
    left: 30px;
    margin-right: 10px;
    color: #fff !important;
    padding: 8px 18px;
    border-radius: 200px;
  }
  .quest-menu-point{
    background-color: #ff1e4a;
  }
  .quest-menu-point{
    background-color: #ff1e4a;
  }
  .rating-menu-point{
    background-color: #1090f5;
  }
  .train-menu-point{
    background-color: #1e8dff;
  }
  .things-menu-point{
    background-color: #11c140;
  }
  .moderation-menu-point, .admin-menu-point{
    background-color: transparent;
    color: black !important;
  } 
  #header-logo{
    color: #393d40 !important;
  }
  .right-part{
    float: right;
    position: relative;
    top: 14px;
    .create-accaunt{
      background-color: transparent;
      border: 1px solid #ffae1c;
      color: #ffae1c !important;
      line-height: 2.5;
      margin-right: 18px;
      &:hover {
        background-color: #ffae1c;
        border-color: #ffae1c;
        color: #fff !important;
      }
    }
    .sign-in{
      font-size: 15px;
      font-weight: 400;
      position: relative;
      top: 2px;
    }
  }
}
#sign-in{
  font-size: 13px;
  position: relative;
  top: 5px;
  padding: 8px 18px;
  background-color: transparent;
  border-radius: 200px;
  border: 1px solid #ff861e;
  color: #ff861e;
}
#header-logo{
  position: relative;
  top: 8px;
  img{
  	width: 40px;
  }
  span{
  	font-family: 'BoomBoom';
  	font-size: 28px;
  	position: relative;
  	bottom: 14px;
  	left: 3px;
  }
}
.signin-button{
  line-height: 2.6 !important;
  background-color: #09da40 !important;
  color: white !important;
}