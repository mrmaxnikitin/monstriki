.btn {
  border: 0;
  cursor: pointer;
  display: inline-block;
  text-align: center;
  white-space: nowrap;
  font-size: 90%;
  line-height: 3;
  padding: 0 2.5em;
  background-color: #00a8c4;
  /*border: 1px solid #09879c;*/
  color: #fff !important;
  border-radius: 100px;
  font-family: "Open Sans", Helvetica, Arial, sans-serif;
  font-weight: 600;
  outline: none !important;
  -webkit-transition: 0.2s ease-in-out;
  transition: 0.2s ease-in-out;
  &:hover, &:focus{
    color: #fff;
    text-decoration: none;
  }
}
